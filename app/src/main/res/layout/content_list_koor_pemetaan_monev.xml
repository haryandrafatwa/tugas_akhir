<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/cardview_list_all_clicked">

    <LinearLayout
        android:background="@color/colorBackgroundNotYet"
        android:id="@+id/container_content"
        android:paddingTop="@dimen/dimen_medium_8dp"
        android:paddingBottom="@dimen/dimen_medium_8dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:text="@string/title_judulta"
            style="@style/content_all_list_title"/>

        <TextView
            android:id="@+id/ctn_koor_pemetaan_textview_judul"
            style="@style/content_all_list_sub_title"/>

        <View
            style="@style/view_all_line_horizontal"/>

        <TextView
            android:text="@string/text_dosen_reviewer"
            style="@style/content_all_list_title" />

        <TextView
            android:id="@+id/ctn_koor_pemetaan_textview_judul_reviewer"
            style="@style/content_all_list_sub_title"/>

    </LinearLayout>

</androidx.cardview.widget.CardView>
